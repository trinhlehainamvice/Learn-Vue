{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "types": [
      "@intlify/unplugin-vue-i18n/messages"
    ],
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@fontawesome/*": [
        "./src/assets/fontawesome/*"
      ],
      "@ui/*": [
        "./src/components/*"
      ],
      "@views/*": [
        "./src/views/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@plugins/*": [
        "./src/plugins/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
    }
  }
}
